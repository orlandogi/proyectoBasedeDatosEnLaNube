{
    editorial:'Biblio',
  }
  {
    _id:0,
    titulo:1,
    precio:1,
    cantidad:1,
    "Nombre Editorial": "$editorial",
    "Total Ganancia":{
      $multiply:['$precio','$cantidad']
    }
  }
  {
    precio:1
  }


  [
    {
      $group:
        {
          _id: "$editorial",
          "Numero Documentos": {
            $count: {}
          }
        }
    },
    {
      $sort:
        {
          "Numeros Documentos": 1
        }
    }
  ]





  [
    {
      $group:
        {
          _id: "$editorial",
          "Numero de Documentos": {
            $count: {}
          },
          media: {
            $avg: "$precio"
          },
          "Precio Maximo": {
            $max: "$precio"
          }
        }
    },
    {
      $sort:

        {
          "Precio Maximo": 1
        }
    }
  ]


  [
    {
      $group:
        {
          _id: "$editorial",
          Numero: {
            $count: {}
          },
          media: {
            $avg: "$precio"
          }
        }
    },
    {
      $set:
        {
          "Media Total": {
            $trunc: ["$media", 2]
          }
        }
    },
    {
      $out:
        "media_editoriales"
    }
  ]
